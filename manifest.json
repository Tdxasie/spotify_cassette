{
    "name": "Spotify Cassette Assistant",
    "description": "Helps you record your cassettes",
    "manifest_version": 3,
    "version": "0.1",
    "content_scripts":[
        {
        "matches":[
            "https://open.spotify.com/playlist/*"
        ],
        "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "scripting"
    ]
}